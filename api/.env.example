# ─── Mandatory variables ───────────────────────────────────────────────────────
EMULATED=true
PORT=3010
NODE_ENV=development

# ─── Google OAuth (if used) ────────────────────────────────────────────────────
GOOGLE_CLIENT_ID=<GOOGLE_CLIENT_ID>
GOOGLE_CLIENT_SECRET=<GOOGLE_CLIENT_SECRET>
LOCAL_GOOGLE_TOKEN=<LOCAL_GOOGLE_TOKEN>

# ─── Database (MySQL) ──────────────────────────────────────────────────────────
DB_HOST=localhost
DB_USER=root
DB_PASS=<DB_PASS>
DB_NAME=codepushdb

# ─── AWS / S3 (or LocalStack) ──────────────────────────────────────────────────
AWS_ACCESS_KEY_ID=<AWS_ACCESS_KEY_ID>
AWS_SECRET_ACCESS_KEY=<AWS_SECRET_ACCESS_KEY>
S3_BUCKET_NAME=<S3_BUCKET_NAME>         # Optional, if using S3
AWS_REGION=<AWS_REGION>                 # Optional, if using S3
S3_ENDPOINT=http://localhost:4566       # Optional, for LocalStack/dev

# ─── Azure Storage (if using Azure) ────────────────────────────────────────────
AZURE_STORAGE_ACCOUNT=
AZURE_STORAGE_ACCESS_KEY=

# ─── Redis ─────────────────────────────────────────────────────────────────────
REDIS_HOST=localhost
REDIS_PORT=6379

# ─── Other (optional) ──────────────────────────────────────────────────────────
DISABLE_ACQUISITION=false
DISABLE_MANAGEMENT=false
ENABLE_ACCOUNT_REGISTRATION=true
UPLOAD_SIZE_LIMIT_MB=200
ENABLE_PACKAGE_DIFFING=false
LOGGING=false
DEBUG_DISABLE_AUTH=false
DEBUG_USER_ID=

# ─── CloudFront (optional, if using with S3) ───────────────────────────────────
CLOUDFRONT_DOMAIN=<CLOUDFRONT_DOMAIN>
CLOUDFRONT_KEY_PAIR_ID=<CLOUDFRONT_KEY_PAIR_ID>